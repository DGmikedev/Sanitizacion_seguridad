					"listen": "test",
					"script": {
						"exec": [
							"// ESTATUS CODE == 200 ??\r",
							"pm.test(\"Status code is 200\", function () { pm.response.to.have.status(200) });\r",
							"\r",
							"// REPONSE\r",
							"var jsonData =            pm.response.json(); \r",
							"var bookingId =           jsonData.bookingid;\r",
							"var lastName =            jsonData.booking.lastname;\r",
							"var firstName =           jsonData.booking.firstname;\r",
							"var totalPrice =          jsonData.booking.totalprice;\r",
							"var depositPaid =         jsonData.booking.depositpaid;\r",
							"var bookingDateCheckIn =  jsonData.booking.bookingdates.checkin;\r",
							"var bookingDateCheckOut = jsonData.booking.bookingdates.checkout;\r",
							"var additionalNeeds =     jsonData.booking.additionalneeds;\r",
							"\r",
							"// BLOQUE DE VERIFICACIÓN DE EXISTENCIA DE DATOS\r",
							"pm.test(\"Veridicando bookingId\",            ()=>{ pm.expect(bookingId).to.not.be.null            });\r",
							"pm.test(\"Vaerificando lastName\",            ()=>{ pm.expect(lastName).to.not.be.empty            });\r",
							"pm.test(\"Vaerificando firstName\",           ()=>{ pm.expect(firstName).to.not.be.empty           });\r",
							"pm.test(\"Vaerificando totalPrice\",          ()=>{ pm.expect(totalPrice).to.not.be.null           });\r",
							"pm.test(\"Vaerificando depositPaid\",         ()=>{ pm.expect(depositPaid).to.not.be.null          });\r",
							"pm.test(\"Vaerificando bookingDateCheckIn\",  ()=>{ pm.expect(bookingDateCheckIn).to.not.be.empty  });\r",
							"pm.test(\"Vaerificando bookingDateCheckOut\", ()=>{ pm.expect(bookingDateCheckOut).to.not.be.empty });\r",
							"pm.test(\"Vaerificando additionalNeeds\",     ()=>{ pm.expect(additionalNeeds).to.not.be.empty     });\r",
							"\r",
							"// BLOQUE DE GUARDADO EN EAMBIENTE DE VALORES RETORNADOS\r",
							"pm.environment.set(\"bookingId\", bookingId);\r",
							"pm.environment.set(\"lastName\", lastName);\r",
							"pm.environment.set(\"firstName\", firstName);\r",
							"pm.environment.set(\"totalPrice\", totalPrice);\r",
							"pm.environment.set(\"depositPaid\", depositPaid);\r",
							"pm.environment.set(\"bookingDateCheckIn\", bookingDateCheckIn);\r",
							"pm.environment.set(\"bookingDateCheckOut\", bookingDateCheckOut);\r",
							"pm.environment.set(\"additionalNeeds\", additionalNeeds);\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
