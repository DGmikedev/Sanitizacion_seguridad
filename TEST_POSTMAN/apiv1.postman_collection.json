		{
			"name": "Get-Booker",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// VERIFICACIÓN CODIGO 200\r",
							"pm.test(\"Status code is 200\", function () { pm.response.to.have.status(200) });\r",
							"\r",
							"// RECUPERACIÓN DE DATA\r",
							"var jsonData =            pm.response.json();\r",
							"//var bookingId =           jsonData.bookingid;\r",
							"var lastName =            jsonData.lastname;\r",
							"var firstName =           jsonData.firstname;\r",
							"var totalPrice =          jsonData.totalprice;\r",
							"var depositPaid =         jsonData.depositpaid;\r",
							"var bookingDateCheckIn =  jsonData.bookingdates.checkin;\r",
							"var bookingDateCheckOut = jsonData.bookingdates.checkout;\r",
							"var additionalNeeds =     jsonData.additionalneeds;\r",
							"\r",
							"// BLOQUE DE COMPARACIÓN DE DATOS INSERTADOS CON DATOS RECUPERADOS\r",
							"pm.test(\"Verificando request de lastName correcta\", ()=>{ \r",
							"    console.log(`- lastName - obtenido:${lastName} vs Guardado: ${pm.environment.get(\"lastName\")}`); \r",
							"    pm.expect(lastName).to.eql(pm.environment.get(\"lastName\")) \r",
							"});\r",
							"\r",
							"pm.test(\"Verificando request de firstName corecta\",  ()=>{ \r",
							"    console.log(`- firstName - obtenido:${firstName} vs Guardado: ${pm.environment.get(\"firstName\")}`); \r",
							"    pm.expect(firstName).to.eql( pm.environment.get(\"firstName\"))\r",
							"});\r",
							"\r",
							"pm.test(\"Verificando request de totalPrice corecta\",  ()=>{ \r",
							"    console.log(` - totalPrice - obtenido:${totalPrice} vs Guardado: ${pm.environment.get(\"totalPrice\")}`) \r",
							"    pm.expect(totalPrice).to.eql( pm.environment.get(\"totalPrice\"))\r",
							"});\r",
							"\r",
							"pm.test(\"Verificando request de depositPaid corecta\",  ()=>{ \r",
							"    console.log(` - depositPaid - obtenido:${depositPaid} vs Guardado: ${pm.environment.get(\"depositPaid\")}`); \r",
							"    pm.expect(depositPaid).to.eql( pm.environment.get(\"depositPaid\"))\r",
							"});\r",
							"pm.test(\"Verificando request de bookingDateCheckIn corecta\",  ()=>{ \r",
							"    console.log(` - bookingDateCheckIn - obtenido:${bookingDateCheckIn} vs Guardado: ${pm.environment.get(\"bookingDateCheckIn\")}`); \r",
							"    pm.expect(bookingDateCheckIn).to.eql( pm.environment.get(\"bookingDateCheckIn\"))\r",
							"});\r",
							"\r",
							"pm.test(\"Verificando request de bookingDateCheckOut corecta\",  ()=>{ \r",
							"    console.log(` - bookingDateCheckOut - obtenido: ${bookingDateCheckOut} vs Guardado: ${pm.environment.get(\"bookingDateCheckOut\")}`); \r",
							"    pm.expect(bookingDateCheckOut).to.eql( pm.environment.get(\"bookingDateCheckOut\"))}\r",
							");\r",
							"\r",
							"pm.test(\"Verificando request de additionalNeeds corecta\",  ()=>{ \r",
							"    console.log(` - additionalNeeds - obtenido: ${additionalNeeds} vs Guardado: ${pm.environment.get(\"additionalNeeds\")}`); \r",
							"    pm.expect(additionalNeeds).to.eql( pm.environment.get(\"additionalNeeds\"))\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
